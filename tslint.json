{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "lib"
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}
